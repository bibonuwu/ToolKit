    <Page.Resources>
        <!-- Define a reusable DropShadowEffect -->
        <DropShadowEffect x:Key="ButtonDropShadowEffect"
                          Color="Gray"
                          Direction="0"
                          BlurRadius="50"
                          ShadowDepth="1"
                          Opacity="0.2" />

        <!-- Define a reusable ControlTemplate for the buttons -->
        <ControlTemplate x:Key="ButtonTemplate" TargetType="{x:Type Button}">
            <Border Name="ContentBorder"
                    Background="#00FFFFFF"
                    CornerRadius="30">
                <Grid>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}"
                               VerticalAlignment="Center"
                               Margin="5"
                               HorizontalAlignment="Center" />
                    </StackPanel>
                </Grid>
            </Border>
            <ControlTemplate.Triggers>
                <Trigger Property="UIElement.IsMouseOver" Value="True">
                    <Setter TargetName="ContentBorder" Property="Background" Value="#60CDFF" />
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>

        <!-- Define a reusable Style for the buttons -->
        <Style x:Key="ButtonStyle" TargetType="{x:Type Button}">
            <Setter Property="Template" Value="{StaticResource ButtonTemplate}" />
            <Setter Property="Height" Value="50" />
            <Setter Property="Width" Value="50" />
            <Setter Property="Background" Value="{x:Null}" />
            <Setter Property="BorderBrush" Value="{x:Null}" />
        </Style>



   
    </Page.Resources>

